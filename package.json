{
    "name": "sso",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test:unit": "jest --testPathPattern=./test/",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs -d src/typeorm.config.ts",
        "migration_auto": "yarn run typeorm migration:generate src/infrastructure/postgres/migrations/auto",
        "migration_make": "typeorm-ts-node-commonjs migration:create",
        "migration_run": "yarn run typeorm migration:run",
        "migration_revert": "yarn run typeorm migration:revert"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^2.0.5",
        "@nestjs/microservices": "^10.3.10",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.10",
        "@nestjs/swagger": "^7.3.0",
        "@nestjs/typeorm": "^10.0.2",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/express-session": "^1.18.0",
        "@types/passport-jwt": "^4.0.1",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "csurf": "^1.11.0",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "helmet": "^7.1.0",
        "install": "^0.13.0",
        "jest-mock-extended": "^3.0.7",
        "lodash": "^4.17.21",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.20",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.0",
        "argon2": "^0.40.1",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-formatter-gitlab": "^5.1.0",
        "eslint-formatter-multiple": "^2.0.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-nestjs": "^1.2.3",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-security": "^2.1.1",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "roots": [
            "<rootDir>/src/",
            "<rootDir>/test/"
        ],
        "modulePaths": [
            "<rootDir>",
            "src"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/test/",
            "<rootDir>/node_modules/"
        ],
        "coverageDirectory": "../coverage",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "cobertura"
        ],
        "testEnvironment": "node",
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "outputDirectory": "./reports/",
        "outputName": "junit.xml",
        "uniqueOutputName": "false",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
    "eslint-formatter-multiple": {
        "formatters": [
            {
                "name": "stylish",
                "output": "console"
            },
            {
                "name": "gitlab"
            },
            {
                "name": "junit",
                "output": "file",
                "path": "reports/eslint-junit-report.xml"
            }
        ]
    }
}
